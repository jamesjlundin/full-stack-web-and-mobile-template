{
  "name": "@acme/rag",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./config": {
      "types": "./src/config.ts",
      "import": "./src/config.ts"
    },
    "./chunk": {
      "types": "./src/chunk.ts",
      "import": "./src/chunk.ts"
    },
    "./embed": {
      "types": "./src/embed.ts",
      "import": "./src/embed.ts"
    },
    "./query": {
      "types": "./src/query.ts",
      "import": "./src/query.ts"
    }
  },
  "scripts": {
